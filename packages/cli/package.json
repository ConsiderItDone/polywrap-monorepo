{
  "name": "@web3api/cli",
  "description": "Web3API CLI",
  "version": "0.0.1-prealpha.5",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/web3-api/monorepo.git"
  },
  "files": [
    "build",
    "bin"
  ],
  "bin": {
    "w3": "bin/w3"
  },
  "scripts": {
    "build": "rimraf ./build && tsc --project tsconfig.build.json",
    "lint": "eslint --color --ext .ts src/",
    "test": "jest --passWithNoTests --runInBand --verbose",
    "test:ci": "jest --passWithNoTests --runInBand --verbose",
    "test:watch": "jest --watch --passWithNoTests --verbose",
    "intl:extract": "formatjs extract 'src/**/*.ts*' --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "intl:compile:en": "formatjs compile lang/en.json --ast --out-file compiled-lang/en.json",
    "intl:compile:es": "formatjs compile lang/es.json --ast --out-file compiled-lang/es.json"
  },
  "dependencies": {
    "@formatjs/intl": "^1.8.2",
    "@web3api/client-js": "0.0.1-prealpha.6",
    "@web3api/client-test-env": "0.0.1-prealpha.6",
    "@web3api/core-js": "0.0.1-prealpha.6",
    "@web3api/ens-plugin-js": "0.0.1-prealpha.6",
    "@web3api/ethereum-plugin-js": "0.0.1-prealpha.6",
    "@web3api/ipfs-plugin-js": "0.0.1-prealpha.6",
    "@web3api/schema-bind": "0.0.1-prealpha.6",
    "@web3api/schema-compose": "0.0.1-prealpha.6",
    "@web3api/schema-parse": "0.0.1-prealpha.6",
    "assemblyscript": "0.17.14",
    "axios": "0.19.2",
    "chalk": "4.1.0",
    "chokidar": "^3.5.1",
    "fs-extra": "9.0.1",
    "gluegun": "4.6.1",
    "graphql-tag": "2.11.0",
    "ipfs-http-client": "^48.1.3",
    "js-yaml": "3.14.0",
    "mustache": "4.0.1",
    "ora": "4.0.0",
    "spawn-command": "0.0.2-1",
    "ws": "7.3.1",
    "intl-messageformat": "^9.5.2",
    "intl-messageformat-parser": "^6.4.2"
  },
  "devDependencies": {
    "@types/jest": "26.0.8",
    "@types/node": "12.6.9",
    "@web3api/test-env-js": "0.0.1-prealpha.6",
    "jest": "26.2.2",
    "rimraf": "3.0.2",
    "ts-jest": "26.1.4",
    "@formatjs/cli": "^3.1.6",
    "@formatjs/ts-transformer": "^3.2.0",
    "typescript": "4.2.3"
  },
  "gitHead": "7346adaf5adb7e6bbb70d9247583e995650d390a",
  "publishConfig": {
    "access": "public"
  }
}
