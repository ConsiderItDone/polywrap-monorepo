{{#scalar}}
{{#toLower}}{{name}}{{/toLower}} = reader
    .read_{{#toLower}}{{#toMsgPack}}{{toGraphQLType}}{{/toMsgPack}}{{/toLower}}().unwrap_or_default();
{{/scalar}}
{{#array}}
{{#toLower}}{{name}}{{/toLower}} = reader
    .read_{{#toLower}}{{#toMsgPack}}{{toGraphQLType}}{{/toMsgPack}}{{/toLower}}(|reader| {{#toWasm}}{{toGraphQLType}}{{/toWasm}} {
        {{> deserialize_array}}
    })
    .expect("Failed to read {{#toLower}}{{name}}{{/toLower}}");
{{/array}}
{{#enum}}
{{> deserialize_enum}}
return value;
{{/enum}}
{{#object}}
{{> deserialize_object}}
return object;
{{/object}}
